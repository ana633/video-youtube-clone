{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";var initialState={currentUser:null,loading:false,error:false};export var userSlice=createSlice({name:\"user\",initialState:initialState,reducers:{loginStart:function loginStart(state){state.loading=true;},loginSuccess:function loginSuccess(state,action){state.loading=false;state.currentUser=action.payload;},loginFailure:function loginFailure(state){state.loading=false;state.error=true;},logout:function logout(state){state.currentUser=null;state.loading=false;state.error=false;},subscription:function subscription(state,action){if(state.currentUser.subscribedUsers.includes(action.payload)){state.currentUser.subscribedUsers.splice(state.currentUser.subscribedUsers.findIndex(function(channelId){return channelId===action.payload;}),1);}else{state.currentUser.subscribedUsers.push(action.payload);}},editUser:function editUser(state,action){state.currentUser.img=action.payload;}}});var _userSlice$actions=userSlice.actions,loginStart=_userSlice$actions.loginStart,loginSuccess=_userSlice$actions.loginSuccess,loginFailure=_userSlice$actions.loginFailure,logout=_userSlice$actions.logout,subscription=_userSlice$actions.subscription,editUser=_userSlice$actions.editUser;export{loginStart,loginSuccess,loginFailure,logout,subscription,editUser};export default userSlice.reducer;","map":{"version":3,"names":["createSlice","initialState","currentUser","loading","error","userSlice","name","reducers","loginStart","state","loginSuccess","action","payload","loginFailure","logout","subscription","subscribedUsers","includes","splice","findIndex","channelId","push","editUser","img","actions","reducer"],"sources":["/Users/anasaldana/Desktop/video-youtube-clone/client/src/redux/userSlice.js"],"sourcesContent":["import { createSlice } from \"@reduxjs/toolkit\";\n\nconst initialState = {\n  currentUser: null,\n  loading: false,\n  error: false,\n};\n\nexport const userSlice = createSlice({\n  name: \"user\",\n  initialState,\n  reducers: {\n    loginStart: (state) => {\n      state.loading = true;\n    },\n    loginSuccess: (state, action) => {\n      state.loading = false;\n      state.currentUser = action.payload;\n    },\n    loginFailure: (state) => {\n      state.loading = false;\n      state.error = true;\n    },\n    logout: (state) => {\n      state.currentUser = null;\n      state.loading = false;\n      state.error = false;\n    },\n    subscription: (state, action) => {\n      if (state.currentUser.subscribedUsers.includes(action.payload)) {\n        state.currentUser.subscribedUsers.splice(\n          state.currentUser.subscribedUsers.findIndex(\n            (channelId) => channelId === action.payload\n          ),\n          1\n        );\n      } else {\n        state.currentUser.subscribedUsers.push(action.payload);\n      }\n    },\n    editUser: (state, action) => {\n      state.currentUser.img = action.payload;\n    \n    }\n  },\n});\n\nexport const { loginStart, loginSuccess, loginFailure, logout, subscription, editUser } =\n  userSlice.actions;\n\nexport default userSlice.reducer;"],"mappings":"AAAA,OAASA,WAAT,KAA4B,kBAA5B,CAEA,GAAMC,aAAY,CAAG,CACnBC,WAAW,CAAE,IADM,CAEnBC,OAAO,CAAE,KAFU,CAGnBC,KAAK,CAAE,KAHY,CAArB,CAMA,MAAO,IAAMC,UAAS,CAAGL,WAAW,CAAC,CACnCM,IAAI,CAAE,MAD6B,CAEnCL,YAAY,CAAZA,YAFmC,CAGnCM,QAAQ,CAAE,CACRC,UAAU,CAAE,oBAACC,KAAD,CAAW,CACrBA,KAAK,CAACN,OAAN,CAAgB,IAAhB,CACD,CAHO,CAIRO,YAAY,CAAE,sBAACD,KAAD,CAAQE,MAAR,CAAmB,CAC/BF,KAAK,CAACN,OAAN,CAAgB,KAAhB,CACAM,KAAK,CAACP,WAAN,CAAoBS,MAAM,CAACC,OAA3B,CACD,CAPO,CAQRC,YAAY,CAAE,sBAACJ,KAAD,CAAW,CACvBA,KAAK,CAACN,OAAN,CAAgB,KAAhB,CACAM,KAAK,CAACL,KAAN,CAAc,IAAd,CACD,CAXO,CAYRU,MAAM,CAAE,gBAACL,KAAD,CAAW,CACjBA,KAAK,CAACP,WAAN,CAAoB,IAApB,CACAO,KAAK,CAACN,OAAN,CAAgB,KAAhB,CACAM,KAAK,CAACL,KAAN,CAAc,KAAd,CACD,CAhBO,CAiBRW,YAAY,CAAE,sBAACN,KAAD,CAAQE,MAAR,CAAmB,CAC/B,GAAIF,KAAK,CAACP,WAAN,CAAkBc,eAAlB,CAAkCC,QAAlC,CAA2CN,MAAM,CAACC,OAAlD,CAAJ,CAAgE,CAC9DH,KAAK,CAACP,WAAN,CAAkBc,eAAlB,CAAkCE,MAAlC,CACET,KAAK,CAACP,WAA
{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";var initialState={currentVideo:null,loading:false,error:false};export var videoSlice=createSlice({name:\"video\",initialState:initialState,reducers:{fetchStart:function fetchStart(state){state.loading=true;},fetchSuccess:function fetchSuccess(state,action){state.loading=false;state.currentVideo=action.payload;},fetchFailure:function fetchFailure(state){state.loading=false;state.error=true;},like:function like(state,action){if(!state.currentVideo.likes.includes(action.payload)){state.currentVideo.likes.push(action.payload);state.currentVideo.dislikes.splice(state.currentVideo.dislikes.findIndex(function(userId){return userId===action.payload;}),1);}},dislike:function dislike(state,action){if(!state.currentVideo.dislikes.includes(action.payload)){state.currentVideo.dislikes.push(action.payload);state.currentVideo.likes.splice(state.currentVideo.likes.findIndex(function(userId){return userId===action.payload;}),1);}},views:function views(state,action){state.currentVideo.views+=action.payload;}}});var _videoSlice$actions=videoSlice.actions,fetchStart=_videoSlice$actions.fetchStart,fetchSuccess=_videoSlice$actions.fetchSuccess,fetchFailure=_videoSlice$actions.fetchFailure,like=_videoSlice$actions.like,dislike=_videoSlice$actions.dislike,views=_videoSlice$actions.views;export{fetchStart,fetchSuccess,fetchFailure,like,dislike,views};export default videoSlice.reducer;","map":{"version":3,"names":["createSlice","initialState","currentVideo","loading","error","videoSlice","name","reducers","fetchStart","state","fetchSuccess","action","payload","fetchFailure","like","likes","includes","push","dislikes","splice","findIndex","userId","dislike","views","actions","reducer"],"sources":["/Users/anasaldana/Desktop/video-youtube-clone/client/src/redux/videoSlice.js"],"sourcesContent":["import { createSlice } from \"@reduxjs/toolkit\";\n\nconst initialState = {\n  currentVideo: null,\n  loading: false,\n  error: false,\n};\n\nexport const videoSlice = createSlice({\n  name: \"video\",\n  initialState,\n  reducers: {\n    fetchStart: (state) => {\n      state.loading = true;\n    },\n    fetchSuccess: (state, action) => {\n      state.loading = false;\n      state.currentVideo = action.payload;\n    },\n    fetchFailure: (state) => {\n      state.loading = false;\n      state.error = true;\n    },\n    like: (state, action) => {\n      if (!state.currentVideo.likes.includes(action.payload)) {\n        state.currentVideo.likes.push(action.payload);\n        state.currentVideo.dislikes.splice(\n          state.currentVideo.dislikes.findIndex(\n            (userId) => userId === action.payload\n          ),\n          1\n        );\n      }\n    },\n    dislike: (state, action) => {\n      if (!state.currentVideo.dislikes.includes(action.payload)) {\n        state.currentVideo.dislikes.push(action.payload);\n        state.currentVideo.likes.splice(\n          state.currentVideo.likes.findIndex(\n            (userId) => userId === action.payload\n          ),\n          1\n        );\n      }\n    },\n    views: (state, action) => {\n      state.currentVideo.views += action.payload;\n    }\n  },\n});\n\nexport const { fetchStart, fetchSuccess, fetchFailure, like, dislike, views } =\n  videoSlice.actions;\n\nexport default videoSlice.reducer;"],"mappings":"AAAA,OAASA,WAAT,KAA4B,kBAA5B,CAEA,GAAMC,aAAY,CAAG,CACnBC,YAAY,CAAE,IADK,CAEnBC,OAAO,CAAE,KAFU,CAGnBC,KAAK,CAAE,KAHY,CAArB,CAMA,MAAO,IAAMC,WAAU,CAAGL,WAAW,CAAC,CACpCM,IAAI,CAAE,OAD8B,CAEpCL,YAAY,CAAZA,YAFoC,CAGpCM,QAAQ,CAAE,CACRC,UAAU,CAAE,oBAACC,KAAD,CAAW,CACrBA,KAAK,CAACN,OAAN,CAAgB,IAAhB,CACD,CAHO,CAIRO,YAAY,CAAE,sBAACD,KAAD,CAAQE,MAAR,CAAmB,CAC/BF,KAAK,CAACN,OAAN,CAAgB,KAAhB,CACAM,KAAK,CAACP,YAAN,CAAqBS,MAAM,CAACC,OAA5B,CACD,CAPO,CAQRC,YAAY,CAAE,sBAACJ,KAAD,CAAW,CACvBA,KAAK,CAACN,OAAN,CAAgB,KAAhB,CACAM,KAAK,CAACL,KAAN,CAAc,IAAd,CACD,CAXO,CAYRU,IAAI,CAAE,cAACL,KAAD,CAAQE,MAAR,CAAmB,CACvB,GAAI,CAACF,KAAK,CAACP,YAAN,CAAmBa,KAAnB,CAAyBC,Q